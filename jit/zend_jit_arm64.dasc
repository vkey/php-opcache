			|	LOAD_ADDR_ZTS FCARG2x, executor_globals, uninitialized_zval
			|	EXT_CALL zend_hash_next_index_insert, REG0
			|	// if (UNEXPECTED(!var_ptr)) {
			|	cbz RETVALx, >1
			|.cold_code
