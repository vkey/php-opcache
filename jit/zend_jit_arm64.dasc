						|	NIY // b &exit_addr
					}
				} else if (false_label != (uint32_t)-1) {
					|	b =>false_label
				}
				if (op1_info & MAY_BE_ANY) {
					if (exit_addr) {
						if (branch_opcode == ZEND_JMPNZ || branch_opcode == ZEND_JMPNZ_EX) {
