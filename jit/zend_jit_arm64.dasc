||	if (GCC_GLOBAL_REGS) {
|		cmp IP, tmp_reg1
||	} else {
|		ldr tmp_reg2, EX->opline
|		cmp tmp_reg2, tmp_reg1
||	}
|.endmacro

|.macro LOAD_ZVAL_ADDR, reg, addr
||	if (Z_MODE(addr) == IS_CONST_ZVAL) {
|		LOAD_ADDR reg, Z_ZV(addr)
||	} else if (Z_MODE(addr) == IS_MEM_ZVAL) {
|		LOAD_BASE_ADDR reg, Z_REG(addr), Z_OFFSET(addr)
||	} else {
||		ZEND_UNREACHABLE();
||	}
|.endmacro

