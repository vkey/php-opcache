				|	bvs >1
				|	SET_ZVAL_TYPE_INFO res_addr, IS_TRUE, TMP1w, TMP2
				|	bne => true_label
				|1:
				|	SET_ZVAL_TYPE_INFO res_addr, IS_FALSE, TMP1w, TMP2
