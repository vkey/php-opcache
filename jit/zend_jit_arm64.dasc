|.macro LOAD_BASE_ADDR, reg, base, offset
||	if (offset) {
||		if (offset > ADD_SUB_IMM) {
|			LOAD_32BIT_VAL reg, offset
|			add reg, Rx(base), reg
||		} else {
|			add reg, Rx(base), #offset
||		}
||	} else {
||		if (base == ZREG_RSP) {
|			mov reg, sp
||		} else {
|			mov reg, Rx(base)
||		}
||	}
|.endmacro

