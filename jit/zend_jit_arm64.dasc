||	} else if (Z_MODE(addr) == IS_MEM_ZVAL) {
|		PUSH_BASE_ADDR Z_REG(addr), Z_OFFSET(addr), tmp_reg
||	} else {
||		ZEND_UNREACHABLE();
||	}
|.endmacro

